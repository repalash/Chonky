    "@types/memoizee": "^0.4.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.33",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.7",
    "@types/redux-watch": "^1.1.1",
    "@types/shortid": "^0.0.32",
