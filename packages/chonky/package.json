{
  "name": "chonky",
  "version": "2.3.2-ij.1",
  "description": "A File Browser component for React",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "prepare": "tsup",
    "format": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore . --write --list-different",
    "format-check": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore --check ."
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.16.4",
    "@mui/material": "^5.15.10",
    "@mui/styles": "^5.16.0",
    "@reduxjs/toolkit": "^2.2.6",
    "date-fns": "^3.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "printWidth": 88
  },
  "module": "dist/index.mjs",
  "size-limit": [
    {
      "path": "dist/chonky.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chonky.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@emotion/react": "^11.11.3",
