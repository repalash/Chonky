    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "classnames": "^2.5.1",
    "deepmerge": "^4.3.0",
    "exact-trie": "^1.0.13",
    "fast-sort": "^3.4.0",
    "filesize": "^10.1.0",
